<?xml version = "1.0" encoding = "UTF-8" ?>
<Application xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:VisibleOnePOS"
             x:Class="VisibleOnePOS.App">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Styles/Colors.xaml" />
                <ResourceDictionary Source="Resources/Styles/Styles.xaml" />
            </ResourceDictionary.MergedDictionaries>
            <Color x:Key="MainBgColor">#32a850</Color>
            <Color x:Key="MemuBgColor">#5c4404</Color>
            <Color x:Key="AppBgColor">#e3c95f</Color>
            <Color x:Key="NavigateColor">#ffffff</Color>
        </ResourceDictionary>
    </Application.Resources>
</Application>
